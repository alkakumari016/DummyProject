<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:context = "http://www.springframework.org/schema/context"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context.xsd" >
	 <bean name="pointA" class="com.cts.auto.Point">
		 <property name="x" value="125"/>
		 <property name="y" value="125"></property>
	 </bean>
	 
	 <bean name="pointB" class="com.cts.auto.Point">
	 	<property name="x" value="175"></property>
	 	<property name="y" value="175"></property>
	 </bean>
	 
	 <bean name="line" class="com.cts.auto.Line" autowire="constructor"></bean>
</beans>